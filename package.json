{
  "name": "issue-forms-creator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:update": "ng test -u",
    "lint": "ng lint",
    "prettier:write": "prettier --write \"src/**/*.{js,json,css,scss,ts,html,component.html}\"",
    "prettier:check": "prettier \"src/**/*.{js,json,css,scss,ts,html,component.html}\" --list-different",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1400.0",
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@briebug/jest-schematic": "^2.1.1",
    "install": "^0.13.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.16",
    "npm": "^8.12.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.4",
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/elements": "^13.3.11",
    "@babel/core": "^7.14.6",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/angular": "^6.1.21",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@types/jasmine": "~4.0.3",
    "@types/jest": "28.1.0",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.1.1",
    "jest": "27.0.0",
    "karma-coverage": "~2.2.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.3.1",
    "typescript": "~4.6.4"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --cache --fix",
    "src/**/*.{js,json,css,scss,ts,html,component.html}": "prettier --write"
  }
}
